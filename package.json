    "@types/node": "24.1.0",
    "@zavoloklom/eslint-config": "workspace:*",
    "@zavoloklom/prettier-config": "workspace:*",
    "eslint": "9.31.0",
    "nx": "21.3.5",
    "prettier": "3.6.2",
